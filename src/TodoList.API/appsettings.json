{
  "Facebook": {
    "GraphApiEndpoint": "https://graph.facebook.com/v3.2"
  },
  "Google": {
    "AccessTokenEndpoint": "https://www.googleapis.com/oauth2/v4/token",
    "UserInfoEndpoint": "https://www.googleapis.com/oauth2/v2/userinfo"
  },
  "Github": {
    "AccessTokenEndpoint": "https://github.com/login/oauth/access_token",
    "UserInfoEndpoint": "https://api.github.com/user"
  },
  "LinkedIn": {
    "AccessTokenEndpoint": "https://www.linkedin.com/oauth/v2/accessToken",
    "IdEndpoint": "https://api.linkedin.com/v2/me",
    "EmailAddressEndpoint": "https://api.linkedin.com/v2/emailAddress?q=members&projection=(elements*(handle~))"
  }
}
